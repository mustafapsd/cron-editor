{
  "name": "cron-editor-app",
  "description": "A cron expression generator to be used in Angular applications",
  "author": {
    "name": "Mustafa Dağlıoğlu",
    "email": "mustafa.psd@yandex.com"
  },
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
